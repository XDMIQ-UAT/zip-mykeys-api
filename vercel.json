{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp-config-generator.html",
      "dest": "/server.js"
    },
    {
      "src": "/mcp-config-generator-v2.html",
      "dest": "/server.js"
    },
    {
      "src": "/generate-token.html",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}




